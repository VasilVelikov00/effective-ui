{
  "name": "@vasilvelikov/effective-ui",
  "version": "0.0.2",
  "description": "Composable, effect-based UI framework built on Effect.ts",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "license": "MIT",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "prepare": "tsup",
    "clean": "rm -rf dist",
    "preview": "vite"
  },
  "keywords": [
    "effect",
    "effect-ts",
    "ui",
    "framework",
    "functional",
    "typescript"
  ],
  "author": "Vasil Velikov",
  "devDependencies": {
    "@types/node": "^22.13.13",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "eslint": "^9.23.0",
    "eslint-plugin-prettier": "^5.2.5",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "prettier": "^3.5.3",
    "tsup": "^8.4.0",
    "typescript": "^5.8.2",
    "vite": "^6.2.3"
  },
  "dependencies": {
    "effect": "^3.14.2",
    "regexparam": "^3.0.0"
  }
}
